{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/main.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerServerAddress": "192.168.188.27:3333",
            "miDebuggerPath": "${workspaceFolder}/toolchains/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb",
            "miDebuggerArgs": "--cd=${workspaceFolder} --interpreter mi -x ${workspaceFolder}/.gdbinit",
            "targetArchitecture": "arm",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "logging": {"engineLogging": true},
            "preLaunchTask": "build",
            "windows": {
                "name": "(gdb) Launch",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceFolder}/build/src/main.elf",
                "miDebuggerPath": "${workspaceFolder}/toolchains/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb.exe"
            }
        }
    ]
}